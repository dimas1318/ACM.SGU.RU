import java.util.*;
import java.io.*;
 
class Solution {
    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int num = in.nextInt();
        int zeros = 0;
        
        while (num % 2 != 1) {
            zeros++;
            num /= 2;
        }
        
        System.out.println(zeros);
    }
}
