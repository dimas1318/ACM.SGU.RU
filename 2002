import java.util.*;
import java.io.*;
 
class Solution {
    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int count = in.nextInt();

        int result = 0;
        for (int i = 0 ; i < count; i++) {
            result += in.nextInt();
        } 

        System.out.println(result);
    }
}
