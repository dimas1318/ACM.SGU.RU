import java.util.*;
import java.io.*;
 
class Solution {
    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int count = in.nextInt();
                
        int[] res = new int[5];
        for (int i = 0; i < count; i++) {
            res[in.nextInt()]++;
        }
        
        for (int i = 0; i < 5; i++) {
            if (res[i] != 0) {
                System.out.println(i + " " +res[i]);
            }
        } 
    }
}
